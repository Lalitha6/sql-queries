CREATE TABLE employee (
    emp_id INT PRIMARY KEY,
    age INT CHECK (age > 0),
    salary DECIMAL(10, 2) CHECK (salary > 0),
    dept_id INT,
    hire_date DATE CHECK (hire_date <= CURRENT_DATE)
);

INSERT INTO employee (emp_id, age, salary, dept_id, hire_date)
VALUES
